    }

    // phần trăm và điểm trung bình của điểm nhân viên
    public double tiLeDiemNV(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
        double tongDiem = 0;
        foreach(var item in list) 
        {
            tongDiem += (double)(item.DiemNhanVien);
        }
        double diemTrungBinh = tongDiem / list.Count;
        double tiLePhanTram = (diemTrungBinh / 5) * 100; // Giả sử điểm tối đa là 5

        return tiLePhanTram;
    }
    public double diemTBDiemNV(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
        double tongDiem = 0;
        foreach (var item in list)
        {
            tongDiem += (double)(item.DiemNhanVien);
        }
        double diemTB = tongDiem / list.Count;
        return Math.Round(diemTB, 1);
    }

    // phần trăm và điểm trung bình của điểm cơ sở
    public double tiLeDiemCS(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
        double tongDiem = 0;
        foreach (var item in list)
        {
            tongDiem += (double)(item.DiemCoSo);
        }
        double diemTrungBinh = tongDiem / list.Count;
        double tiLePhanTram = (diemTrungBinh / 5) * 100; // Giả sử điểm tối đa là 5

        return tiLePhanTram;
    }
    public double diemTBDiemCS(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
