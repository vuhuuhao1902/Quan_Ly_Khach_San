@functions {
    QuanLyKhachSanContext db = new QuanLyKhachSanContext();

    public List<string> getAllHinhAnh_Of_KS(int id)
    {
        var query = from k in db.KhachSans
                    join p in db.Phongs on k.Id equals p.IdKhachSan
                    join h in db.HinhAnhs on p.Id equals h.IdPhong
                    where k.Id == id
                    select h.UrlHinhAnh;
        var result = query.ToList();
        return result;
    }

    public List<string> getUrlHinhAnhById(int id)
    {
        List<string> hinh = db.HinhAnhs.Where(h => h.Id == id).Select(h => h.UrlHinhAnh).ToList();

        return hinh;
    }

    public double? getGiaTrungBinhKS(int idKs)
    {
        List<Phong> phongs = db.Phongs.Where(p => p.IdKhachSan == idKs).ToList();

        double? giaTB = 0;

        foreach (var item in phongs)
        {
            giaTB += item.GiaPhong;
        }

        return giaTB / phongs.Count();
    }

    // lấy ra ảnh tài khoản
    //public string getAnhOfTaiKhoanById(int? id)
    //{
    //    var taikhoan = db.TaiKhoans.FirstOrDefault(t => t.Id == id);
    //    string anh = taikhoan.Anh;
    //    return anh;
    //} 

    // lấy ra tài khoản
    public TaiKhoan getTaiKhoanById(int? id)
    {
        var taikhoan = db.TaiKhoans.FirstOrDefault(t => t.Id == id);
        return taikhoan;
    }

    // lấy tất cả tiện nghi của phòng
    public List<TienNghi> getAllTienNghiOfPhong(int? idP)
    {
        // Kiểm tra idP có giá trị không
        if (idP == null) return new List<TienNghi>();  // Trả về danh sách rỗng nếu idP là null

        var result = db.PhongTienNghis
               .Where(ptn => ptn.IdPhong == idP) // Sử dụng tham số idP
               .Join(db.TienNghis,
                     ptn => ptn.IdTienNghi,
                     tn => tn.Id,
                     (ptn, tn) => tn)  // Trả về các đối tượng TienNghi
               .ToList();  // Chuyển kết quả thành List

        return result;
    }

    //
    public int getCountPhongByIdKs(int idKs)
    {
        var query = from k in db.KhachSans
                    join p in db.Phongs on k.Id equals p.IdKhachSan
                    where k.Id == idKs
                    group p by k.Id into g
                    select g.Count();

        return query.FirstOrDefault();
    }

    // lấy danh sách đánh giá của khách sạn
    public List<DanhGium> getAllDanhGiaByIdKs(int idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).OrderByDescending(t => t.Id).ToList();
        return list;
    }

    // tính điểm trung bình
    public double getDiemTBOfDanhGia(int? diemSachSe, int? diemThoaiMai, int? diemNhanVien, int? diemCoSo)
    {
        double diemTB = (double)(diemSachSe + diemThoaiMai + diemNhanVien + diemCoSo) / 4; 
        return diemTB;
    }

    // tính điểm trung bình all
    public double getDiemTBAllById(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
        double diemTB = 0;
        foreach(var item in list)
        {
            diemTB += (double)(item.DiemThoaiMai + item.DiemNhanVien + item.DiemSachSe + item.DiemCoSo) / 4;
        }
        diemTB = diemTB / list.Count;
        return Math.Round(diemTB, 1);
    }

    // đánh giá bằng chữ
    public string getDanhGiaBangChu(double diem)
    {
        var result = "";
        if (diem >= 0 && diem <= 3)
        {
            result = "Rất tệ";
        }
        else if (diem > 3 && diem <= 3.5)
        {
            result = "Tệ";
        }
        else if (diem > 3.5 && diem <= 4)
        {
            result = "Bình thường";
        }
        else if (diem > 4 && diem <= 4.5)
        {
            result = "Tốt";
        }
        else
        {
            result = "Rất tốt";
        }
        return result;
    }

    // phần trăm và điểm trung bình của điểm sạch sẽ
    public double tiLeDiemSS(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
        double tongDiem = 0;
        foreach(var item in list) 
        {
            tongDiem += (double)(item.DiemSachSe);
        }
        double diemTrungBinh = tongDiem / list.Count;
        double tiLePhanTram = (diemTrungBinh / 5) * 100; // Giả sử điểm tối đa là 5

        return tiLePhanTram;
    }
    public double diemTBDiemSS(int? idKs)
    {
        var list = db.DanhGia.Where(d => d.IdKhachSan == idKs).ToList();
        double tongDiem = 0;
